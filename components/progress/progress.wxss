/* components/progress/progress.wxss */
.circle_box,.circle_draw{ position: relative;
  width:100%;
  height:100%;
}
.circle_bg{position: absolute;}
.circle_box{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.circle_txt{
  position: absolute;
  font-size: 28rpx;
}
.circle_image{
    position: absolute;
    width:100%;
    height:100%;
    border-radius: 50%;
}