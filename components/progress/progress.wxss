/* components/progress/progress.wxss */
.circle_box,
.circle_draw {
  position: relative;
  z-index: 0;
  width: 100%;
  height: 100%;
}

.circle_bg {
  z-index: 0;
  position: absolute;
}

.circle_box {
  z-index: 0;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.circle_txt {
  position: absolute;
  font-size: 28rpx;
}

.circle_image {
  position: absolute;
  width: 100%;
  height: 100%;
  border-radius: 50%;
}